{
  "name": "api-java-service",
  "root": "apps/backend/api-java-service",
  "sourceRoot": "apps/backend/api-java-service/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "./gradlew build",
        "cwd": "apps/backend/api-java-service"
      },
      "outputs": [
        "{projectRoot}/build"
      ]
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "./gradlew test",
        "cwd": "apps/backend/api-java-service"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "./gradlew bootRun",
        "cwd": "apps/backend/api-java-service"
      }
    },
    "clean": {
      "executor": "nx:run-commands",
      "options": {
        "command": "./gradlew clean",
        "cwd": "apps/backend/api-java-service"
      }
    }
  },
  "tags": []
}
