    "@fastify/type-provider-typebox": "^4.1.0",
    "dotenv": "^16.3.1",
    "fastify": "^4.25.0",
    "pg": "^8.11.3",
    "uuid": "^9.0.1",
    "@sinclair/typebox": "^0.32.30"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^9.0.7",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0",
